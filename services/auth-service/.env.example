# Example environment variables for the auth-service
# Copy this file to .env and fill in the values.

# Port for the service to run on
PORT=3001

# Database connection string
DATABASE_URL="postgresql://user:password@localhost:5432/auth"

# Secret for signing JSON Web Tokens (JWTs)
JWT_SECRET="a-very-secret-and-strong-key-that-you-should-change"
